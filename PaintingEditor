	local Paintings = 0
	local function addcanvaseditoroption(instance)
		local TextButton = Instance.new("TextButton")
		local UICorner_4 = Instance.new("UICorner")
		TextButton.Parent = ScrollingFrame
		TextButton.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
		TextButton.Position = UDim2.new(0, 0, -1.02620137, 0)
		TextButton.Size = UDim2.new(0.97299999, 0, 0.0930019692, 0)
		TextButton.Font = Enum.Font.GothamSemibold
		Paintings += 1
		TextButton.Text = "Painting"..tostring(Paintings)
		TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextButton.TextScaled = true
		TextButton.TextSize = 14.000
		TextButton.TextWrapped = true
		UICorner_4.Parent = TextButton

		TextButton.MouseButton1Click:Connect(function()
			if instance.Parent.Canvas:FindFirstChild("Image") then
				instance.Parent.Canvas["Image"].Texture = "http://www.roblox.com/asset/?id="..tonumber(TextBox.Text)
			elseif instance.Parent.Canvas:FindFirstChild("FrontFace") then
				instance.Parent.Canvas.FrontFace.Frame.ImageLabel.Image = "http://www.roblox.com/asset/?id="..tonumber(TextBox.Text)
			end
		end)
	end


	local RoAdmin = Instance.new("ImageButton")
