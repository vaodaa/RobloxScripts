	RoAdmin.ZIndex = 3
	RoAdmin.Image = "rbxassetid://1496271525"
	RoAdmin.ImageColor3 = Color3.fromRGB(176, 0, 0)
	RoAdmin.MouseEnter:Connect(function()
		game:GetService("Players").LocalPlayer.PlayerGui.HUD.Phone.Screen.Tooltip.Text = "Painting Editor"
		game:GetService("Players").LocalPlayer.PlayerGui.HUD.Phone.Screen.Tooltip.Shadow.Text = "Painting Editor"
		game:GetService("Players").LocalPlayer.PlayerGui.HUD.Phone.Screen.MetaFrame.Apps1.RoAdmin.ImageColor3 = Color3.fromRGB(141, 6, 6)
	end)
	RoAdmin.MouseLeave:Connect(function()
		game:GetService("Players").LocalPlayer.PlayerGui.HUD.Phone.Screen.Tooltip.Text = ""
		game:GetService("Players").LocalPlayer.PlayerGui.HUD.Phone.Screen.Tooltip.Shadow.Text = ""
		game:GetService("Players").LocalPlayer.PlayerGui.HUD.Phone.Screen.MetaFrame.Apps1.RoAdmin.ImageColor3 = Color3.fromRGB(176, 0, 0)
	end)
	RoAdmin.MouseButton1Click:Connect(function()
		local UI = ToyShop
		if UI.Enabled == true then
			UI.Enabled = false
		else
			UI.Enabled = true
		end
	end)
	Icon.Name = "Icon"
